{
  "backend": {
    "language": "none",
    "javaKt": {
      "management": "",
      "packagePath": ""
    },
    "php": {
      "modRewritePhpExtension": false,
      "serverAsApi": false
    }
  },
  "frontend": {
    "buildDir": "dist",
    "ghUser": "inpercima",
    "installDependencies": true,
    "language": "en",
    "modules": {
      "enabled": true,
      "features": {
        "defaultRoute": "dashboard",
        "name": "features"
      },
      "notFound": {
        "enabled": false,
        "name": ""
      },
      "routing": false
    },
    "packageJson": {
      "contributors": [{
        "name": "Marcel Hendrich",
        "email": "inpercima@gmail.com"
      }],
      "homepage": "http://mc-status.inpercima.net/",
      "repository": "https://github.com/inpercima/mc-status/"
    },
    "prefix": "mcs",
    "theme": "indigo-pink",
    "useGoogleFonts": true,
    "useYarn": true
  },
  "general": {
    "author": "Marcel Hendrich",
    "description": "Shows the status of a minecraft server.",
    "modRewriteIndex": false,
    "name": "mc-status",
    "title": "Minecraft server status",
    "useDocker": false,
    "useMITLicense": true,
    "useMock": false,
    "useSecurity": false
